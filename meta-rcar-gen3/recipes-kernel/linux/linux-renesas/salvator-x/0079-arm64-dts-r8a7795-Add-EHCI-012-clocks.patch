From 573ff47b213ee6095ab88495154d84443a0d4a54 Mon Sep 17 00:00:00 2001
From: Kazuya Mizuguchi <kazuya.mizuguchi.ks@renesas.com>
Date: Wed, 19 Aug 2015 13:20:54 +0900
Subject: [PATCH 79/85] arm64: dts: r8a7795: Add EHCI[012] clocks

Signed-off-by: Kazuya Mizuguchi <kazuya.mizuguchi.ks@renesas.com>
---
 arch/arm64/boot/dts/renesas/r8a7795.dtsi  |    8 ++++++--
 include/dt-bindings/clock/r8a7795-clock.h |    3 +++
 2 files changed, 9 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/renesas/r8a7795.dtsi b/arch/arm64/boot/dts/renesas/r8a7795.dtsi
index 11418fa..98fbd5c 100644
--- a/arch/arm64/boot/dts/renesas/r8a7795.dtsi
+++ b/arch/arm64/boot/dts/renesas/r8a7795.dtsi
@@ -262,17 +262,21 @@
 					reg = <0 0xe615014c 0 4>, <0 0xe61501c4 0 4>;
 					clocks = <&s2d1_clk>, <&s2d1_clk>, <&s2d1_clk>,
 						 <&s2d1_clk>, <&s2d1_clk>, <&s2d1_clk>,
-						 <&s2d1_clk>, <&p_clk>;
+						 <&s2d1_clk>, <&p_clk>, <&p_clk>,
+						 <&p_clk>, <&p_clk>;
 					#clock-cells = <1>;
 					clock-indices = <
 						R8A7795_CLK_DU3 R8A7795_CLK_DU2
 						R8A7795_CLK_DU1 R8A7795_CLK_DU0
 						R8A7795_CLK_LVDS R8A7795_CLK_HDMI1
 						R8A7795_CLK_HDMI0 R8A7795_CLK_HSUSB
+						R8A7795_CLK_EHCI0 R8A7795_CLK_EHCI1
+						R8A7795_CLK_EHCI2
 					>;
 					clock-output-names =
 						"du3", "du2", "du1", "du0", "lvds0",
-						"hdmi1","hdmi0", "hsusb";
+						"hdmi1","hdmi0", "hsusb", "ehci0",
+						"ehci1", "ehci2";
 				};
 
 				mstp9_clks: mstp9_clks@e6150994 {
diff --git a/include/dt-bindings/clock/r8a7795-clock.h b/include/dt-bindings/clock/r8a7795-clock.h
index de1381c..e1d4718 100644
--- a/include/dt-bindings/clock/r8a7795-clock.h
+++ b/include/dt-bindings/clock/r8a7795-clock.h
@@ -52,6 +52,9 @@
 #define R8A7795_CLK_HDMI1		28
 #define R8A7795_CLK_HDMI0		29
 #define R8A7795_CLK_HSUSB		4
+#define R8A7795_CLK_EHCI0		3
+#define R8A7795_CLK_EHCI1		2
+#define R8A7795_CLK_EHCI2		1
 
 /* MSTP8 */
 #define R8A7795_CLK_AVB		12
-- 
