 		.slave_id	= SHDMA_SLAVE_SDHI2_RX,
-		.addr		= 0xee140030 + 0x2000,
+		.addr		= 0xee160030 + 0x2000,
 		.chcr		= CHCR_RX(XMIT_SZ_16BIT),
-		.mid_rid	= 0xc2,
+		.mid_rid	= 0xd4,
 	}, {
 		.slave_id	= SHDMA_SLAVE_MMC_TX,
 		.addr		= 0xee200034,
